		public void reverse(String x)
		{
			// declaring the input as a string
			String word = x;
			// making the length an integer to help reverse
			int len = word.length();
			// for loop to print the index backwards
			for (int i = len-1; i >= 0; i--)
			{
				// output to user
				System.out.print(word.charAt(i));
			}
			
		}
