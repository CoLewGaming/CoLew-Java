public class testTingTings {

	public static void main(String[] args) {
		
		int number = 9;
		
		int[][] pyr = new int[number*2-1][number+1];
		
		for(int i = number; i > 0; i--)
		{
			for(int j = 0; j < number; j++)
			{
				if(j > i)
				pyr[i][j] = i;
				
				System.out.print(pyr[i][j]);
				
			}
			System.out.println();
		}
		
		
		
		
	}

}
